    "import sqlite3\n",
    "#making datbase\n",
    "conn= sqlite3.connect(\"books.db\")\n",
    "#making cursor\n",
    "c= conn.cursor()\n",
    "c.execute(\"SELECT * FROM allbooks\")\n",
    "\n",
    "item=c.fetchmany(0)\n",
    "for items in item:\n",
    "    print(f\"This book {items[0]} is a great book\")\n",
    "\n",
    "\n",
    "conn.commit()\n",
    "\n",
    "conn.close()\n"
   ]
